{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "asuspended/accordion-content",
  "version": "1.0.0",
  "title": "Accordion Content",
  "category": "design",
  "icon": "text",
  "description": "The collapsible content area of an accordion item.",
  "parent": [ "asuspended/accordion-item" ],
  "supports": {
    "html": false,
    "color": {
      "background": true,
      "text": true,
      "gradients": true
    },
    "spacing": {
      "padding": true,
      "margin": true
    },
    "__experimentalBorder": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "className": true
  },
  "attributes": {
    "overridePadding": {
      "type": "boolean",
      "default": false
    },
    "overrideAnimation": {
      "type": "boolean",
      "default": false
    }
  },
  "textdomain": "advanced-accordion"
}
